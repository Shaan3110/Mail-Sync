{
  "short_name": "HR Manager",
  "name": "hrManager",
  "version": "0.1.0",
  "description": "Your HR Manager",
  "background": {
    "service_worker": "background.js"
  },
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "favicon.ico",
    "48": "logo192.png",
   "128": "logo512.png"
  },
  "permissions": [
    "tabs"
  ],
  "action": {
    "default_title": "HR Manager",
    "default_popup": "index.html"
  }
}
